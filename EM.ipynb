   "source": [
    "%matplotlib inline\n",
    "import matplotlib.pyplot as plt\n",
    "import numpy as np\n",
    "import scipy as sp\n",
    "import scipy.stats as st\n",
    "from collections import Counter\n",
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 81,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Setting up the constants\n",
    "w = 6 # length of motif\n",
    "N = 20 # number of sequences\n",
    "len_lu = [w,w+10] # lower and upper bound on the lengths of sequences\n",
    "m = 4 # alphabet size\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 14,
   "metadata": {},
   "outputs": [],
   "source": [
    "# generating theta\n",
    "def gen_theta(w,m):\n",
    "    theta = [[0]*m]*(w+1)\n",
    "    for i in range(w+1):\n",
    "        th = np.concatenate(([0],st.uniform.rvs(0,1,m-1),[1]))\n",
    "        th = np.sort(th)\n",
    "        theta[i] = np.diff(th)\n",
    "    return theta"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 82,
   "metadata": {},
   "outputs": [],
   "source": [
    "# generating RVs and sequences\n",
    "\n",
    "# generating one sequence\n",
    "def genSeq(n,p): \n",
    "    # Generate an iid random sequences of length n over \n",
    "    # the alphabet 0,1,...,|p|-1 where the probability of i is p[i]\n",
    "    y = [0] * n\n",
    "    for i in range(n):\n",
    "        r = st.uniform.rvs()\n",
    "        y[i] = np.sum(r>np.cumsum(p))\n",
    "    return y\n",
    "\n",
    "# generate N sequences containing motifs at random positions\n",
    "def genSeqMotif(theta,N,len_lu):\n",
    "    w = len(theta) - 1\n",
    "    Y = [[]]*N # sequences\n",
    "    Z = [0]*N # start positions of motifs\n",
    "    L = [0]*N # lengths of sequences\n",
    "    for i in range(N):\n",
    "        L[i] = st.randint.rvs(len_lu[0],len_lu[1]+1)\n",
    "        Z[i] = st.randint.rvs(0,L[i]-w+1)\n",
    "        y_nonmotif_pre = genSeq(Z[i],theta[0])\n",
    "        y_motif = [genSeq(1,theta[i+1])[0] for i in range(w)]     \n",
    "        y_nonmotif_post = genSeq(L[i]-Z[i]-w, theta[0])\n",
    "        Y[i] = np.concatenate((y_nonmotif_pre,y_motif,y_nonmotif_post)).astype(int)\n",
    "    return (Y,Z)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 83,
   "metadata": {},
   "outputs": [],
   "source": [
    "# estimators\n",
    "\n",
    "# Below is the estimator with complete data. You can use some of this code for the incomplete data. However while here we \n",
    "# know the value of z as z=Z[i], for EM we need to do the computation for all possible z and take expectation. Understanding \n",
    "# completeDataML is a good first step for writing the EM code\n",
    "def completeDataML(Y,Z,w,m):\n",
    "    thetaHatML = [[0]*m] * (w+1)\n",
    "    hn = np.array([[0]*m] * (w+1)) # composition for one sequence\n",
    "    h = np.array([[0]*m] * (w+1)) # compostion for all sequences\n",
    "    N = len(Y)\n",
    "    for n in range(N):\n",
    "        z = Z[n]\n",
    "        y = Y[n]\n",
    "        y_nonmotif = np.concatenate((y[0:z],y[z+w:])).astype(int)# nonmotif symbols of each sequence Yk\n",
    "        y_motif = y[z:z+w]# motif symbols of each sequence Yk\n",
    "        hn[0] = [Counter(y_nonmotif)[j] for j in range(m)] #   h(B)  :   (n1, n2, n3, n4)  count nonmotif symbols of each Yk\n",
    "        for i in range(1,w+1):\n",
    "            hn[i] = [Counter([y_motif[i-1]])[j] for j in range(m)] #   h(M(i)) :   (n1, n2, n3, n4)  count symbols of each position in each Yk's motif\n",
    "        h += hn                      #   h(B) and h(M(1))~h(M(w)) :   (n1, n2, n3, n4)  count symbols of each Yk\n",
    "    thetaML = [h[i]/sum(h[i]) for i in range(w+1)]\n",
    "    return thetaML\n",
    "    \n",
    "\n",
    "\n",
    "    \n",
    "    \n",
    "    \n",
    "def compute_all_wnl(theta_current, Y):\n",
    "    N = len(Y)\n",
    "    W_all = [[]] * N\n",
    "    regularized_W = [[]] * N\n",
    "    Ln_prime = [0] * N\n",
    "    for n in range(N):\n",
    "        Ln = len(Y[n])\n",
    "        Ln_prime[n] = Ln - w + 1\n",
    "        w_seq = [1.0] * (Ln_prime[n])\n",
    "        W_all[n] = w_seq\n",
    "        w_seq2 = [0.0] * (Ln_prime[n])\n",
    "        regularized_W[n] = w_seq2\n",
    "    for n in range(N):\n",
    "        y = Y[n]\n",
    "        Wnz_init = 1.0\n",
    "        ####################### theta0 ^ h(Yk)\n",
    "        hn0 = [Counter(y)[j] for j in range(m)]  # = (n1, n2, n3, n4)  count all symbols in Yk\n",
    "        for d in range(m):\n",
    "            Wnz_init *= np.power(theta_current[0][d], hn0[d])\n",
    "        ####################### theta1~6 ^ h(Ykz)\n",
    "        for z in range(0, Ln_prime[n]):\n",
    "            W_all[n][z] = Wnz_init\n",
    "            y_motif = y[z:z + w]  # motif symbols of each sequence Yk\n",
    "            for i in range(1, w + 1):\n",
    "                symbol = y_motif[i - 1]\n",
    "                for j in range(m):\n",
    "                    if symbol == j:\n",
    "                        W_all[n][z] *= theta_current[i][j]/theta_current[0][j]\n",
    "    #Wk\n",
    "    Wk = [0] * N\n",
    "    for n in range(N):\n",
    "        Wk[n] = 0.0\n",
    "        for l in range(0, Ln_prime[n]):\n",
    "            Wk[n] += W_all[n][l]\n",
    "\n",
    "    # wkl/Wk\n",
    "    for n in range(N):\n",
    "        for l in range(0, Ln_prime[n]):\n",
    "            if Wk[n] == 0.0:\n",
    "                regularized_W[n][l] = 0.0\n",
    "            else:\n",
    "                regularized_W[n][l] = W_all[n][l] / Wk[n]\n",
    "\n",
    "    #print(\"W : \", np.matrix(regularized_W))\n",
    "    return regularized_W\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "def E_step(theta_current, Y):\n",
    "    h = np.array([[0.0] * m] * (w + 1))  # all sequences\n",
    "    regularized_W = compute_all_wnl(theta_current, Y)\n",
    "    ############################## Expect_h(M(i)) ###########################\n",
    "    for n in range(N):\n",
    "        y = Y[n]\n",
    "        len_y_prime = len(regularized_W[n])\n",
    "        for l in range(len_y_prime):\n",
    "            weight=regularized_W[n][l]\n",
    "            hnl = np.array([[0.0] * m] * (w + 1)) # each k and each l\n",
    "            for i in range(w):\n",
    "                symbol = y[l + i]\n",
    "                for d in range(m):\n",
    "                    if symbol == d:\n",
    "                        hnl[i + 1][d] = weight\n",
    "            h += hnl\n",
    "    #print(\"Expect h(M(i)) = \", np.matrix(h))\n",
    "\n",
    "    # sum up h(M(i))\n",
    "    sum_hMi = [0] * m\n",
    "    for i in range(1, w + 1):\n",
    "        sum_hMi += h[i]\n",
    "    ############################# Expect_h(B) #############################\n",
    "    for n in range(N):\n",
    "        y = Y[n]\n",
    "        h0 = [Counter(y)[j] for j in range(m)]  # = (n1, n2, n3, n4)  count all symbols in Yk\n",
    "        h[0] += h0\n",
    "    for d in range(m):\n",
    "        h[0][d] = h[0][d] - sum_hMi[d]\n",
    "    #print(\"Expect h(B) = \", np.matrix(h))\n",
    "\n",
    "    return h\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "def M_step(Expect_h):\n",
    "    theta_next = 0\n",
    "    theta_next = [Expect_h[i] / sum(Expect_h[i]) for i in range(w + 1)]\n",
    "    return theta_next\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "# EM motif discovery, stating with theta_start and doing itr_max iterations\n",
    "def motifDiscoveryEM(Y, theta_start, itr_max):\n",
    "    theta_current = theta_start\n",
    "    w = len(theta_current) - 1\n",
    "    m = len(theta_current[0])\n",
    "    N = len(Y)\n",
    "    Expect_hn = np.array([[0] * m] * (w + 1))\n",
    "    for itr in range(10):\n",
    "        # solution\n",
    "        # This code segment could get long. It may be helpful to package some of it in a function\n",
    "        Expect_h = E_step(theta_current, Y)\n",
    "        theta_current = M_step(Expect_h)\n",
    "    return (theta_current)"
   ]
  },
